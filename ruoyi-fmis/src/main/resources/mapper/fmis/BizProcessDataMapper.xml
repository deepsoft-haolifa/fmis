            <if test="string15 != null  and string15 != ''"> and string15 = #{string15}</if>
            <if test="string16 != null  and string16 != ''"> and string16 = #{string16}</if>
            <if test="string17 != null  and string17 != ''"> and string17 = #{string17}</if>
            <if test="string18 != null  and string18 != ''"> and string18 = #{string18}</if>
            <if test="string19 != null  and string19 != ''"> and string19 = #{string19}</if>
            <if test="string20 != null  and string20 != ''"> and string20 = #{string20}</if>
            <if test="datetime1 != null "> and datetime1 = #{datetime1}</if>
            <if test="datetime2 != null "> and datetime2 = #{datetime2}</if>
            <if test="datetime3 != null "> and datetime3 = #{datetime3}</if>
            <if test="datetime4 != null "> and datetime4 = #{datetime4}</if>
            <if test="datetime5 != null "> and datetime5 = #{datetime5}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>


            <!-- string2作为角色参数 1=销售 2=销售经理 3=区域经理 4=副总 5=老总-->
            <!--flow_status 0=待上报 1=已上报 2=销售经理同意 -2=不同意 3=区域经理同意 -3=不同意 4=副总同意 -4=不同意 5=老总同意 -5=不同意 -->
            <if test="roleType == 1 ">
                <if test="queryStatus == 1 ">
                    and q.flow_status in ('0','-2','-3','-4','-5')
                </if>
                <if test="queryStatus == 2 ">
                    and q.flow_status != '0'
                </if>
                ${params.dataScope}
            </if>
            <!-- 销售经理 查看已上报的 -->
            <if test="roleType == 2 ">
                <if test="queryStatus == 0 ">
                    and q.flow_status in ('1','2','-2','3','-3','-4','-5')
                </if>
                <if test="queryStatus == 1 ">
                    and q.flow_status in ('1')
                </if>
                <if test="queryStatus == 2 ">
                    and q.flow_status in ('2','-2','3','-3','-4','-5','4','5')
                </if>
                ${params.dataScope}
            </if>
            <!-- 区域经理  查看 销售经理同意的  -->
            <if test="roleType == 3 ">
                <if test="queryStatus == 0 ">
                    and q.flow_status in ('2','3','-3','4','-4','-5')
                </if>
                <if test="queryStatus == 1 ">
                    and q.flow_status in ('2')
                </if>
                <if test="queryStatus == 2 ">
                    and q.flow_status in ('3','-3','4','-4','-5','5')
                </if>
                ${params.dataScope}
            </if>

            <!-- 副总  查看 区域经理同意的  -->
            <if test="roleType == 4 ">
                <if test="queryStatus == 0 ">
                    and q.flow_status in ('3','4','-4','-5')
                </if>
                <if test="queryStatus == 1 ">
                    and q.flow_status in ('3')
                </if>
                <if test="queryStatus == 2 ">
                    and q.flow_status in ('4','-4','-5','5')
                </if>

                ${params.dataScope}
            </if>
            <!-- 总经理  查看 副总同意的  -->
            <if test="roleType == 5 ">
                <if test="queryStatus == 0 ">
                    and q.flow_status in ('4','5','-5')
                </if>
                <if test="queryStatus == 1 ">
                    and q.flow_status in ('4')
                </if>
                <if test="queryStatus == 2 ">
                    and q.flow_status in ('5','-5')
                </if>

                ${params.dataScope}
            </if>
            <!-- 查询全部 已经完成的 -->
            <if test="roleType == 0 ">
                and q.flow_status=q.normal_flag
            </if>
        </where>
    </select>



